lambda = 0.1*ones(d,1);

%Gamma(2, theta)-hyperprior on theta
psi = 1;
theta = gampdf(1,2,psi + sum(lambda));

%Gamma(2, theta)-prior on the intensitetes/lambda
lambda = gampdf(1:d,2,theta);

%Metropolis-Hasting algorithm
rho = 1;
R = rho*(t(i+1)-t(i-1));
tstar =  t - R + (2*R)*rand(d, 1);


